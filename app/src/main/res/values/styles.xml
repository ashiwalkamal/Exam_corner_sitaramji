<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppThemeone" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>


    <style name="Textview" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/colorblack</item>
        <item name="fontFamily">@font/poppinsmedium</item>
        <item name="android:textSize">@dimen/texts</item>

    </style>

    <style name="welcome" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/colorgray</item>
        <item name="fontFamily">@font/poppinsmedium</item>
        <item name="android:textSize">@dimen/texts</item>

    </style>



